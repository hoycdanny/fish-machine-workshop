    <script>
        // ç®¡ç†é¢æ¿å³æ™‚æ•¸æ“šæ›´æ–°
        const AdminDashboard = {
            // æ¯ç§’æ›´æ–°çµ±è¨ˆæ•¸æ“š
            updateInterval: 1000,
            
            // çµ±è¨ˆæ•¸æ“šçµæ§‹
            stats: {
