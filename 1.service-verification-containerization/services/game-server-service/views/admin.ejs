        // æ›´æ–°è¨˜æ†¶é«”è¨ˆç®—èªªæ˜
        function updateMemoryCalculation() {
            const containerMemoryMB = 768; // æ›´æ–°ç‚º 768MB
            const maxFishCount = parseInt(document.getElementById('maxFishCount').value);
            const memoryPerFish = parseInt(document.getElementById('memoryPerFish').value);
            
            // è¨ˆç®—æ¯æ¢é­šçš„ MB
            const memoryPerFishMB = (containerMemoryMB * memoryPerFish / 100).toFixed(2);
            
            // è¨ˆç®—ç¸½è¨˜æ†¶é«”æ¶ˆè€—
            const estimatedMemoryMB = Math.round(maxFishCount * memoryPerFishMB);
            const estimatedPercent = Math.round((estimatedMemoryMB / containerMemoryMB) * 100);
            
            // æ›´æ–°é¡¯ç¤º
            document.getElementById('memoryPerFishMB').textContent = memoryPerFishMB;
            document.getElementById('maxFishDisplay').textContent = maxFishCount;
            document.getElementById('estimatedMemory').textContent = estimatedMemoryMB;
            document.getElementById('estimatedPercent').textContent = estimatedPercent;
        }
        
        // åˆ‡æ› Demo æ¨¡å¼
        async function toggleDemoMode() {
            const toggle = document.getElementById('demoModeToggle');
            const enabled = toggle.checked;
            const statusText = document.getElementById('demoModeStatus');
            const configDiv = document.getElementById('demoModeConfig');
            
            try {
                const response = await fetch('/admin/api/demo-mode/toggle', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({ enabled: enabled })
                });
                
                const result = await response.json();
                
                if (result.success) {
                    statusText.textContent = enabled ? 'å•Ÿç”¨' : 'é—œé–‰';
                    statusText.style.color = enabled ? '#FF9800' : '#666';
                    configDiv.style.display = enabled ? 'block' : 'none';
                    
                    showDemoModeStatus(`Demo æ¨¡å¼å·²${enabled ? 'å•Ÿç”¨' : 'é—œé–‰'}`, 'success');
                } else {
                    toggle.checked = !enabled;
                    showDemoModeStatus('Demo æ¨¡å¼åˆ‡æ›å¤±æ•—: ' + result.message, 'error');
                }
            } catch (error) {
                console.error('Error toggling demo mode:', error);
                toggle.checked = !enabled;
                showDemoModeStatus('Demo æ¨¡å¼åˆ‡æ›å¤±æ•—: ç¶²è·¯éŒ¯èª¤', 'error');
            }
        }
        
        // æ›´æ–° Demo æ¨¡å¼é…ç½®
        async function updateDemoConfig() {
            const maxFishCount = parseInt(document.getElementById('maxFishCount').value);
            const memoryPerFish = parseInt(document.getElementById('memoryPerFish').value);
            
            console.log('[Admin] æ›´æ–° Demo é…ç½®:', { maxFishCount, memoryPerFish });
            
            try {
                const response = await fetch('/admin/api/demo-mode/config', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({
                        maxFishCount: maxFishCount,
                        memoryPerFish: memoryPerFish
                    })
                });
                
                const result = await response.json();
